{
	"name": "sveltekit-blog-mdx",
	"version": "2.0.0",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run generate:manifest && vite build",
		"preview": "vite preview",
		"check": "svelte-check --fail-on-hints",
		"check:watch": "svelte-check --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
		"format": "prettier --write --plugin-search-dir=. .",
		"generate:images": "node ./generate-responsive-image-data.js",
		"generate:manifest": "node ./generate-manifest.js",
		"generate:sitemap": "node ./generate-sitemap.js",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.0.3",
		"@fontsource/lato": "^4.5.8",
		"@fontsource/lora": "^4.5.10",
		"@fontsource/source-sans-pro": "^4.5.10",
		"@iconify/svelte": "^2.2.1",
		"@rodneylab/sveltekit-components": "^0.2.13",
		"@sveltejs/adapter-static": "1.0.0-next.39",
		"@sveltejs/kit": "1.0.0-next.431",
		"@types/object-hash": "^2.2.1",
		"commitlint": "^17.0.3",
		"dayjs": "^1.11.3",
		"dotenv": "^16.0.1",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"front-matter": "^4.0.2",
		"husky": "^8.0.1",
		"leaflet": "^1.8.0",
		"mdsvex": "^0.10.6",
		"object-hash": "^3.0.0",
		"postcss": "^8.4.14",
		"postcss-html": "^1.4.1",
		"prettier": "~2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"reading-time": "1.4.0",
		"sass": "^1.53.0",
		"sharp": "^0.30.7",
		"stylelint": "^14.9.1",
		"stylelint-config-recommended-scss": "^7.0.0",
		"stylelint-config-standard": "^28.0.0",
		"stylelint-scss": "^4.2.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"svelte-preprocess-import-assets": "^0.1.6",
		"vanilla-lazyload": "^17.8.2",
		"vite": "3.0.9",
		"vite-imagetools": "^4.0.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/sveltekit-blog-mdx"
	},
	"bugs": {
		"url": "https://github.com/rodneylab/sveltekit-blog-mdx/issues"
	},
	"type": "module",
	"peerDependencies": {
		"leaflet": "^1.8.0",
		"vite": "3.0.0"
	}
}
