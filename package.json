{
	"name": "sveltekit-blog-mdx",
	"version": "2.0.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --fail-on-hints",
		"check:watch": "svelte-check --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
		"format": "prettier --write --plugin-search-dir=. .",
		"generate:images": "node ./generate-responsive-image-data.js",
		"generate:sitemap": "node ./generate-sitemap.js",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.3.0",
		"@fontsource/lato": "^4.5.10",
		"@fontsource/lora": "^4.5.12",
		"@fontsource/source-sans-pro": "^4.5.11",
		"@sveltejs/adapter-static": "1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@types/object-hash": "^3.0.2",
		"commitlint": "^17.3.0",
		"dayjs": "^1.11.7",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"front-matter": "^4.0.2",
		"husky": "^8.0.2",
		"iconify-icon": "^1.0.2",
		"mdsvex": "^0.10.6",
		"object-hash": "^3.0.0",
		"postcss": "^8.4.20",
		"postcss-html": "^1.5.0",
		"prettier": "~2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"reading-time": "1.4.0",
		"sass": "^1.57.1",
		"sharp": "^0.31.3",
		"stylelint": "^14.16.1",
		"stylelint-config-recommended-scss": "^8.0.0",
		"stylelint-config-standard": "^29.0.0",
		"stylelint-scss": "^4.3.0",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.0",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^4.0.3",
		"vite-imagetools": "^4.0.13"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/sveltekit-blog-mdx"
